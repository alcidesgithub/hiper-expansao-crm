{
  "name": "temp_app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "db:migrate": "prisma migrate deploy",
    "db:seed": "prisma db seed",
    "db:setup": "npm run db:migrate && npm run db:seed",
    "lint": "eslint",
    "test": "tsx --test src/lib/__tests__/availability.test.ts src/lib/__tests__/availability.integration.test.ts src/lib/__tests__/crm-availability.integration.test.ts src/lib/__tests__/contact-validation.test.ts src/lib/__tests__/funnel-gate.integration.test.ts",
    "test:roles": "tsx --test src/lib/__tests__/lead-select.test.ts src/lib/__tests__/crm-leads-role.integration.test.ts src/lib/__tests__/crm-lead-nested-role.integration.test.ts src/lib/__tests__/crm-pipeline-role.integration.test.ts src/lib/__tests__/crm-meetings-role.integration.test.ts src/lib/__tests__/crm-upload-role.integration.test.ts",
    "test:teams": "tsx --test src/lib/__tests__/teams.integration.test.ts src/lib/__tests__/teams-webhook.integration.test.ts src/lib/__tests__/teams-subscriptions-route.integration.test.ts src/lib/__tests__/teams-subscriptions-sync-route.integration.test.ts",
    "test:e2e:install": "playwright install chromium",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:critical": "npm run test:roles && tsx --test src/lib/__tests__/availability.integration.test.ts src/lib/__tests__/crm-availability.integration.test.ts src/lib/__tests__/teams.integration.test.ts src/lib/__tests__/teams-webhook.integration.test.ts src/lib/__tests__/teams-subscriptions-route.integration.test.ts src/lib/__tests__/teams-subscriptions-sync-route.integration.test.ts src/lib/__tests__/contact-validation.test.ts src/lib/__tests__/funnel-gate.integration.test.ts",
    "release:check": "npm run lint && npx tsc --noEmit && npm run test:critical && npx prisma migrate status",
    "release:check:full": "npm run release:check && npm run test:e2e"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^18.0.1",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.19.2",
    "@tanstack/react-query": "^5.90.21",
    "bcryptjs": "^3.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "googleapis": "^171.4.0",
    "ioredis": "^5.9.2",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "next-auth": "^5.0.0-beta.30",
    "prisma": "^6.19.2",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-email": "^5.2.8",
    "react-hook-form": "^7.71.1",
    "recharts": "^3.7.0",
    "resend": "^6.9.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
